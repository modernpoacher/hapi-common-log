{
  "name": "@modernpoacher/hapi-common-log",
  "version": "1.0.339",
  "description": "Transforms a Hapi request to a Common Log Format log line",
  "keywords": [
    "hapi",
    "common",
    "log",
    "format",
    "common log format"
  ],
  "main": "./index.js",
  "author": "Raquel VÃ©lez <raquel@rckbt.me>",
  "contributors": [
    "Jonathan Perry <modernpoacher@modernpoacher.com> (Modern Poacher Limited)"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:modernpoacher/hapi-common-log.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "build": "npm run babel",
    "build:watch": "npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test mocha test --recursive -r @babel/register",
    "gulp": "gulp --color",
    "pre-commit": "npm run lint && npm run test && npm run build && git add lib",
    "post-commit": "cross-env DEBUG=@modernpoacher/hooks,@modernpoacher/hooks:* npm run gulp -- post-commit"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "moment": "^2.29.4",
    "moment-strftime": "^0.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@modernpoacher/hooks": "^1.0.246",
    "babel-plugin-module-resolver": "^4.1.0",
