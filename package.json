    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "gulp": "^4.0.2",
    "husky": "^7.0.4",
    "mocha": "^9.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^13.0.1"
