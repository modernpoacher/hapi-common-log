    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "gulp": "^4.0.2",
    "husky": "^8.0.2",
    "mocha": "^10.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.2"
